
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ipv6 '0'
	option proto 'static'
	list ipaddr '192.168.1.63/24'
	option gateway '192.168.1.250'
	option ifname 'eth0'

config interface 'wwan'
	option proto 'static'
	option gateway '192.168.1.250'
	list ipaddr '192.168.1.64/24'
	option metric '10'
