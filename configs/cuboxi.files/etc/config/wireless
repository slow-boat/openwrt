config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/soc/20300000.mmcnr/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option htmode 'HT20'
	option disabled '1'

config wifi-iface 'wifinet1'
	option ssid 'myssid'
	option key 'mykey'
	option device 'radio0'
	option mode 'sta'
	option network 'wifi'
	option encryption 'psk2'
