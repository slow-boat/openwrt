config alsa
	option chans_in '2'
	option chans_out '6'
	option rate '48000'
	option period_sz '64'
	option periods '4'
	option format 'S24_3LE'

config rtprio

config gpio
	option amp_gpio '504'
	option amp_hold '90'

config zita-n2j

config crossover
	option ch_start '2 0'
	option duplicate '1'
	option freq '120'
	option gain '-18 3'
	option enabled '1'

config convolver
	option gain '0.4'
	option file '/etc/sound/impulse48.wav'
	option ch_start '-1'
	option enabled '1'
